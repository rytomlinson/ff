    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "node-pg-migrate": "^6.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
